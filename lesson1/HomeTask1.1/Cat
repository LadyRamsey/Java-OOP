package com.gmail.gunkophotos;

public class Cat {
	private String color;
	private double weight;
	private int age;
	private boolean sex;
	private String ration;
	private String name;
	private String breed;
	
	public Cat(String color, double weight, int age, boolean sex, String ration, String name, String breed) {
		super();
		this.color = color;
		this.weight = weight;
		this.age = age;
		this.sex = sex;
		this.ration = ration;
		this.name = name;
		this.breed = breed;
	}

	public Cat() {
		super();
	}
	
	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public double getWeight() {
		return weight;
	}

	public void setWeight(double weight) {
		this.weight = weight;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public boolean isSex() {
		return sex;
	}

	public void setSex(boolean sex) {
		this.sex = sex;
	}

	public String getRation() {
		return ration;
	}

	public void setRation(String ration) {
		this.ration = ration;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getBreed() {
		return breed;
	}

	public void setBreed(String breed) {
		this.breed = breed;
	}

	public void getVoice() {
		System.out.println("Meow-Meow-Meow!!!");
	}

	@Override
	public String toString() {
		return "Cat [color=" + color + ", weight=" + weight + ", age=" + age + ", sex=" + sex + ", ration=" + ration
				+ ", name=" + name + ", breed=" + breed + "]";
	}
	
	
}
